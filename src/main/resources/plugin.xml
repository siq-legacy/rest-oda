<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="%oda.data.source.id"
         point="org.eclipse.datatools.connectivity.oda.dataSource">
      <dataSource
            driverClass="oda.datatools.connectivity.rest.siq.impl.RESTDriver"
            defaultDisplayName="%data.source.name"
            setThreadContextClassLoader="false"
            odaVersion="3.2"
            id="%oda.data.source.id">
            
            <properties>
            
            <propertyGroup
               defaultDisplayName="Connection Properties"
               name="connectionProperties">
            <property
                  name="OdaDriverClass"
                  allowsEmptyValueAsNull="true"
                  defaultDisplayName="User Name"
                  type="string"
                  isEncryptable="false">
            </property>
            <property
                  name="username"
                  allowsEmptyValueAsNull="true"
                  defaultDisplayName="User Name"
                  type="string"
                  isEncryptable="false">
            </property>
            <property
                  name="password"
                  allowsEmptyValueAsNull="true"
                  defaultDisplayName="Password"
                  type="string"
                  isEncryptable="true">
            </property>
              <property
                  name="ipaddress"
                  allowsEmptyValueAsNull="false"
                  defaultDisplayName="IP Address"
                  type="string"
                  isEncryptable="false">
            </property>
             <property
                  name="port"
                  allowsEmptyValueAsNull="false"
                  defaultDisplayName="Port"
                  type="string"
                  isEncryptable="false">
            </property>
             <property
                  name="app"
                  allowsEmptyValueAsNull="false"
                  defaultDisplayName="Port"
                  type="string"
                  isEncryptable="false"
                 >
            </property>
           </propertyGroup>
         </properties>
         
         
      </dataSource>
      <dataSet
            defaultDisplayName="%data.set.name"
            id="oda.datatools.connectivity.rest.siq.dataSet">
        <dataTypeMapping
               nativeDataTypeCode="1"
               odaScalarDataType="String"
               nativeDataType="text">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="4"
               odaScalarDataType="Integer"
               nativeDataType="integer">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="8"
               odaScalarDataType="Double"
               nativeDataType="float">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="3"
               odaScalarDataType="Decimal"
               nativeDataType="bigdecimal">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="91"
               odaScalarDataType="Date"
               nativeDataType="Date">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="92"
               odaScalarDataType="Time"
               nativeDataType="Time">
         </dataTypeMapping>
          <dataTypeMapping
               nativeDataTypeCode="98"
               odaScalarDataType="JavaObject"
               nativeDataType="datetime">
         </dataTypeMapping>
         <dataTypeMapping
               
               nativeDataTypeCode="93"
               odaScalarDataType="Timestamp"
               nativeDataType="Timestamp;">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="16"
               odaScalarDataType="Boolean"
               nativeDataType="boolean">
         </dataTypeMapping>
        
          <dataTypeMapping
               nativeDataTypeCode="11"
               odaScalarDataType="JavaObject"
               nativeDataType="javaObject">
         </dataTypeMapping>
          <dataTypeMapping
               nativeDataTypeCode="10"
               odaScalarDataType="String"
               nativeDataType="uuid">
         </dataTypeMapping>
         
      </dataSet>
      <dataSet
            defaultDisplayName="%data.set.name.UI"
            id="oda.datatools.connectivity.rest.siq.dataSet.UI">
        <dataTypeMapping
               nativeDataTypeCode="1"
               odaScalarDataType="String"
               nativeDataType="text">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="4"
               odaScalarDataType="Integer"
               nativeDataType="integer">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="8"
               odaScalarDataType="Double"
               nativeDataType="float">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="3"
               odaScalarDataType="Decimal"
               nativeDataType="bigdecimal">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="91"
               odaScalarDataType="Date"
               nativeDataType="Date">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="92"
               odaScalarDataType="Time"
               nativeDataType="Time">
         </dataTypeMapping>
          <dataTypeMapping
               nativeDataTypeCode="98"
               odaScalarDataType="JavaObject"
               nativeDataType="datetime">
         </dataTypeMapping>
         <dataTypeMapping
               
               nativeDataTypeCode="93"
               odaScalarDataType="Timestamp"
               nativeDataType="Timestamp;">
         </dataTypeMapping>
         <dataTypeMapping
               nativeDataTypeCode="16"
               odaScalarDataType="Boolean"
               nativeDataType="boolean">
         </dataTypeMapping>
        
          <dataTypeMapping
               nativeDataTypeCode="11"
               odaScalarDataType="JavaObject"
               nativeDataType="javaObject">
         </dataTypeMapping>
          <dataTypeMapping
               nativeDataTypeCode="10"
               odaScalarDataType="String"
               nativeDataType="uuid">
         </dataTypeMapping>
      </dataSet>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.connectionProfile">
      <category
            name="%data.source.name"
            parentCategory="org.eclipse.datatools.connectivity.oda.profileCategory"
            id="%oda.data.source.id">
      </category>
      <connectionProfile
            pingFactory="org.eclipse.datatools.connectivity.oda.profile.OdaConnectionFactory"
            name="%connection.profile.name"
            category="%oda.data.source.id"
            id="%oda.data.source.id">
      </connectionProfile>
      <connectionFactory
            name="ODA Connection Factory"
            profile="%oda.data.source.id"
            class="org.eclipse.datatools.connectivity.oda.profile.OdaConnectionFactory"
            id="org.eclipse.datatools.connectivity.oda.IConnection">
      </connectionFactory>
   </extension>

</plugin>
